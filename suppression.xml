if (positionInFibSequence == 0 || positionInFibSequence == 1) {
	if (positionInFibSequence <= 2) {
		return 1;
	} else {
		if (positionInFibSequence == 0 || positionInFibSequence == 2) {
			return positionInFibSequence;
		}
		
		int prevFib = 0;
		int currentFib = 0;
		
		for (int i=2; i <positionInFibSequence; i++) {
			int temp = prevFib + currentFib;
			prevFib = currentFib;
			currentFib = temp;
		}
		return currentFib;
	}	
} 